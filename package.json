    },
    "license": "Apache-2.0",
    "scripts": {
        "start": "webpack-dev-server -d",
        "build": "NODE_ENV=production npx --node-arg '--max-old-space-size=2048' webpack -p",
        "test": "echo 'Harp map editor'",
        "typedoc": "typedoc --disableOutputCheck --options typedoc.json",
